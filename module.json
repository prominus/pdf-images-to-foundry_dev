{
    "id": "starfinder-pdf-to-foundry",
    "name": "starfinder-pdf-to-foundry",
    "title": "Starfinder PDF to Foundry",
    "description": "This is a quick and dirty script that lets you take PDFs that contain certain adventures, and export them to a format that is compatible with Foundry Virtual Tabletop for the Starfinder system.",
    "version": "0.0.1",
    "authors": [
        {
            "name": "prominus",
            "discord": "prominus001#5173"
        }
    ],
    "compatibility": {
        "minimum": "10",
        "verified": "10"
    },
    "packs": [
        {
            "name": "mapping",
            "label": "Image Mapping",
            "path": "packs/mapping.db",
            "entity": "JournalEntry",
            "type": "JournalEntry",
            "system": "starfinder"
        }
    ],
    "esmodules": ["esmodules/index.js"],
    "styles": ["styles/styles.css"],
    "manifest": "https://gitlab.com/encounterlibrary/my-content-module/-/raw/main/module.json",
    "download": "https://gitlab.com/encounterlibrary/my-content-module/-/raw/main/my-content-module.zip",
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "lang/en.json"
        }
    ],
    "system": "starfinder",
    "url": "https://github.com/prominus/starfinderpdftofoundry",
    "license": "LICENSE",
    "readme": "README"
  }
